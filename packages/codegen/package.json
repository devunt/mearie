{
  "name": "@mearie/codegen",
  "version": "0.1.5",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@logtape/logtape": "^2.0.2",
    "@mearie/config": "workspace:*",
    "@mearie/native": "workspace:*",
    "@mearie/shared": "workspace:*",
    "picocolors": "^1.1.1",
    "picomatch": "^4.0.3",
    "tinyglobby": "^0.2.15"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/picomatch": "^4.0.2",
    "@vue/compiler-sfc": "^3.5.29",
    "svelte": "^5.53.3",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.mts",
          "default": "./dist/index.mjs"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/index.cjs"
        }
      },
      "./package.json": "./package.json"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.mts"
  }
}
