{
  "name": "@mearie/cli",
  "version": "0.1.4",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/cli.ts"
  },
  "bin": {
    "mearie": "./src/cli.ts"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@mearie/codegen": "workspace:*",
    "@mearie/config": "workspace:*",
    "citty": "^0.2.1"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "publishConfig": {
    "access": "public",
    "bin": {
      "mearie": "./dist/cli.mjs"
    },
    "exports": {
      ".": {
        "import": {
          "types": "./dist/cli.d.mts",
          "default": "./dist/cli.mjs"
        },
        "require": {
          "types": "./dist/cli.d.cts",
          "default": "./dist/cli.cjs"
        }
      },
      "./package.json": "./package.json"
    },
    "main": "./dist/cli.cjs",
    "module": "./dist/cli.mjs",
    "types": "./dist/cli.d.mts"
  }
}
