{
  "name": "@mearie/vite",
  "version": "0.0.1-next.3",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@mearie/codegen": "workspace:*",
    "@mearie/config": "workspace:*"
  },
  "devDependencies": {
    "tsdown": "catalog:",
    "tsx": "^4.20.6",
    "typescript": "catalog:",
    "vite": "^7.1.12"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./package.json": "./package.json"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts"
  }
}
